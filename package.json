{
  "name": "my-razzle-app",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "analyze": "BUNDLE_ANALYZE=true razzle build",
    "db:migrate": "babel-node node_modules/.bin/knex migrate:latest",
    "db:seed": "NODE_ENV=development babel-node node_modules/.bin/knex seed:run",
    "db:create": "knex migrate:make $TABLE"
  },
  "engines": {
    "node": ">=11.0.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "knex": "^0.19.1",
    "path": "^0.12.7",
    "pg": "^7.12.0",
    "postgresql": "^0.0.1",
    "razzle": "^3.0.0",
    "razzle-plugin-long-term-caching": "^1.0.0-alpha.30",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.2",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "morgan": "^1.9.1",
    "razzle-plugin-bundle-analyzer": "^1.0.2",
    "razzle-plugin-compression": "^1.0.0",
    "razzle-plugin-scss": "^3.0.0",
    "razzle-plugin-scss-extended": "^1.0.1-1"
  }
}
